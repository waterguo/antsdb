# Set root logger level to DEBUG and its only appender to A1.
log4j.rootLogger=DEBUG, console, file

# Console appender
log4j.appender.console=org.apache.log4j.ConsoleAppender
log4j.appender.console.layout=org.apache.log4j.PatternLayout
log4j.appender.console.layout.ConversionPattern=%d{dd HH:mm:ss,SSS} [%t] %-5p %c{2} - %m%n
log4j.appender.console.Threshold=info

# File appender
log4j.appender.file=org.apache.log4j.RollingFileAppender
log4j.appender.file.MaxBackupIndex=5
log4j.appender.file.append=true
log4j.appender.file.file=logs/fish.log
log4j.appender.file.layout=org.apache.log4j.PatternLayout
log4j.appender.file.layout.ConversionPattern=%d{dd HH:mm:ss,SSS} [%t] %-5p %c{2} - %m%n

# trace appender
log4j.appender.sql=org.apache.log4j.RollingFileAppender
log4j.appender.sql.MaxBackupIndex=5
log4j.appender.sql.append=true
log4j.appender.sql.file=logs/sql.log
log4j.appender.sql.layout=org.apache.log4j.PatternLayout
log4j.appender.sql.layout.ConversionPattern=%d{dd HH:mm:ss,SSS} [%t] %-5p %c{2} - %m%n
log4j.appender.sql.Threshold=TRACE

# synclog trace appender
log4j.appender.synclog=org.apache.log4j.RollingFileAppender
log4j.appender.synclog.MaxBackupIndex=5
log4j.appender.synclog.append=true
log4j.appender.synclog.file=logs/synclog.log
log4j.appender.synclog.layout=org.apache.log4j.PatternLayout
log4j.appender.synclog.layout.ConversionPattern=%d{dd HH:mm:ss,SSS} [%t] %-5p %c{2} - %m%n
log4j.appender.synclog.Threshold=TRACE

# other categories
log4j.logger.org.apache=INFO
log4j.logger.org.apache.zookeeper=WARN
log4j.logger.org.apache.hadoop=WARN
log4j.logger.org.apache.parquet=WARN
log4j.logger.io.netty=INFO
